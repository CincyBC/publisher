{
  "name": "@malloy-publisher/sdk",
  "description": "Malloy Publisher SDK",
  "version": "0.0.1",
  "type": "module",
  "main": "dist/index.cjs.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc && vite build --mode ${NODE_ENV:=development}",
    "clean": "rm -rf ./dist && rm -rf ./node_modules && rm -f package-lock.json",
    "lint": "eslint ./src --ext .ts,.tsx --ignore-path .gitignore --fix",
    "format": "prettier --write --parser typescript '**/*.{ts,tsx}'",
    "analyze": "vite-bundle-visualizer",
    "generate-api-types": "openapi-generator-cli generate -i ../../api-doc.yaml -g typescript-axios -o src/client/"
  },
  "peerDependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.0",
    "@emotion/react": "^11.13.3",
    "@emotion/styled": "^11.13.0"
  },
  "dependencies": {
    "@malloydata/malloy": "^0.0.211",
    "@malloydata/render": "^0.0.211",
    "@mui/icons-material": "^6.0.1",
    "@mui/material": "^6.0.1",
    "@mui/x-tree-view": "^7.16.0",
    "@react-spring/web": "^9.7.4",
    "@vitejs/plugin-react": "^4.3.1",
    "axios": "^1.7.7",
    "markdown-to-jsx": "^7.5.0",
    "mdast-comment-marker": "^2.1.2",
    "typescript": "4.7.4",
    "unified": "^11.0.5",
    "vite": "^5.3.5",
    "vite-plugin-dts": "^4.0.0",
    "vite-plugin-svgr": "^4.2.0",
    "@tanstack/react-query": "^5.59.16"
  },
  "devDependencies": {
    "@openapitools/openapi-generator-cli": "^2.13.5",
    "@types/react": "^18.3.1",
    "@typescript-eslint/eslint-plugin": "^8.0.1",
    "@typescript-eslint/parser": "^8.0.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.35.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-storybook": "^0.8.0",
    "openapi-typescript": "^6.7.6",
    "prettier": "^3.3.3",
    "shiki": "^1.16.3",
    "styled-components": "^6.1.12",
    "typescript": "^5.5.4",
    "vite-bundle-visualizer": "^1.2.1"
  }
}
